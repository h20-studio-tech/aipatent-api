    "def run_simple_request(q: EvalQuestion, n_return_vals=5):\n",
    "    results = (\n",
    "        document_table.search(q.question_with_context).select([\"chunk_id\"]).limit(n_return_vals).to_list()\n",
    "    )\n",
    "    return [str(q.chunk_id) == str(r[\"chunk_id\"]) for r in results]"
   ]
  },
