   "source": [
    "with open(rf\"C:\\Users\\vtorr\\Work\\Projects\\aipatent\\evals\\synthetic_eval_cleaned_gvhd_ald.json\", \"r\") as f:\n",
    "    synthetic_questions = json.load(f)\n",
    "synthetic_questions[:5]\n",
    "eval_questions = [EvalQuestion(**question) for question in synthetic_questions]\n",
    "eval_questions[0]"
   ]
  },
