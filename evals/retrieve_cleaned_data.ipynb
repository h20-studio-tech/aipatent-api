    "from pydantic import BaseModel\n",
    "\n",
    "class Chunk(BaseModel):\n",
    "    element_id: str\n",
    "    text: str\n",
    "    page_number: int\n",
    "    filename: str\n",
    "    chunk_id: int\n",
    "    \n",
    "class ChunkReview(BaseModel):\n",
    "    relevant: bool\n",
    "    \n",
    "class ReviewedChunk(BaseModel):\n",
    "    element_id: str\n",
    "    text: str\n",
    "    page_number: int\n",
    "    filename: str\n",
    "    chunk_id: int\n",
    "    relevant: bool\n",
    "\n"
